{
    "name": "uselauncher/backup-cli",
    "description": "Launcher's CLI to backup databases and sites.",
    "homepage": "https://uselauncher.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/uselauncher/backup-cli/issues",
        "source": "https://github.com/uselauncher/backup-cli"
    },
    "require": {
        "php": "^8.0",
        "illuminate/encryption": "^8.68",
        "illuminate/http": "^8.0",
        "laravel-zero/framework": "^8.8",
        "league/flysystem": "^1.1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-rackspace": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "pbmedia/laravel-webdav": "^1.10",
        "spatie/db-dumper": "^3.0",
        "spatie/flysystem-dropbox": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "application"
    ]
}